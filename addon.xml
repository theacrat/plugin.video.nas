<addon id="plugin.video.nas" name="NAS - Not a Stremio" provider-name="Xyl" version="1.0.2">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
        <import addon="script.module.requests" version="2.19.1"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="resources/lib/plugin_source.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.service" library="resources/lib/service.py"/>
    <extension point="xbmc.python.module" library="resources/lib/"/>
    <extension point="xbmc.addon.metadata">
        <reuselanguageinvoker>true</reuselanguageinvoker>
        <summary lang="en">Use your Stremio setup on Kodi.</summary>
        <description lang="en">This addon pulls your library, catalogs, streams, and more from your Stremio account.</description>
        <platform>all</platform>
        <license/>
        <assets>
            <icon>resources/media/nas_icon.png</icon>
        </assets>
    </extension>
</addon>